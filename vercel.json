{
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxt/vercel-builder",
      "config": {}
    },
    {
      "src": ".storybook/build.js",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/_nuxt/.+",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
