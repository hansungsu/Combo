{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "npm run build-storybook",
    "devCommand": "npm run storybook",
    "installCommand": "npm install",
    "framework": null,
    "outputDirectory": "./storybook-static",
    "routes": [
        { "src": "/storybook/(.*)", "dest": "/storybook/$1", "headers": { "x-vercel-cache": "server" } },
        { "src": "/", "dest": "/index.html", "headers": { "x-vercel-cache": "server" } }
    ],
    "rewrites": [
    { "source": "/storybook/(.*)", "destination": "/storybook" }
    ]
  }